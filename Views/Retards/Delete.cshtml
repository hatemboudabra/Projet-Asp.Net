@model BibliothequeVieuxMontreal.Models.Retard

@{
    ViewData["Title"] = "Delete";
}


<h3>Êtes vous sûr ?</h3>
<div>
    <h4>Supprimer retard</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdMembre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdMembre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdLivre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdLivre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nbjour)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nbjour)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DatePret)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DatePret)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> 
        <a asp-action="Index" class="btn btn-primary">Retour à la liste</a>
    </form>
</div>
